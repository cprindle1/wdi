<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <header>
      <h1>Stores Index</h1>

    <nav>
      <ul>
        <li>
          <a href='/'>Home</a>
        </li>
        <li>
          <a href='/store/new'>Create a New Store</a>
        </li>
      </ul>
    </nav>
  </header>
  <main>
    <h2>List of stores</h2>
  <ul>
      <% for(var i = 0; i < store.length; i++){ %>
          <li>
            <a href="/store/<%=store[i]._id%>"><%=store[i].name%></a>
          </li>
      <% } %>
  </ul>
</main>
  </body>
</html>
